{% extends 'base.html.twig' %}
{% block title %}Choisissez un nouveau mot de passe{% endblock %}
{% block body %}
<div class="container">
    <h3>Choisissez un nouveau mot de passe</h3><br>
    {{ form_start(form)}}
        {{ form_errors(form) }}
        {{ form_row(form.password) }}
        <button type="submit" class="btn btn-primary">Valider</button>
    {{ form_end(form) }}
</div>
{% endblock  %}